<blockTableStyle id="contenido">
    <rowStyle start="0" stop="-1" height="30"/>
    <!-- Aplica bordes a todas las celdas de la tabla -->
    <lineStyle kind="GRID" colorName="#0a2756" start="1,0" stop="1,0" thickness="0.5" />
    <lineStyle kind="GRID" colorName="#0a2756" start="3,0" stop="3,0" thickness="0.5" />
        <!-- Quita las líneas de las celdas 1 y 3 (deja sin bordes) -->
    <lineStyle kind="GRID" colorName="rgba(255, 255, 255, 0)" start="0,0" stop="0,0" thickness="0"/>
    <lineStyle kind="GRID" colorName="rgba(255, 255, 255, 0)" start="2,0" stop="2,0" thickness="0"/>
</blockTableStyle>

